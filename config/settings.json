{
    "project_name": "BOT-vCSGO-Beta",
    "version": "2.0.0",
    "language": "es",
    "proxy_settings": {
        "enabled": false,
        "timeout": 30,
        "max_retries": 3,
        "rotation_enabled": true,
        "retry_delay": 2
    },
    "database": {
        "enabled": false,
        "type": "sqlite",
        "path": "csgo_arbitrage.db"
    },
    "scrapers": {
        "default_interval": 300,
        "default_timeout": 30
    },
    "update_intervals": {
        "steam": 3600,
        "fast_platforms": 60,
        "slow_platforms": 300
    },
    "logging": {
        "level": "INFO",
        "format": "{time:YYYY-MM-DD HH:mm:ss} | {level} | {name} | {message}",
        "rotation": "1 day",
        "retention": "7 days"
    }
}